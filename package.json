{
  "name": "wey",
  "version": "0.3.5",
  "main": "lib/main.js",
  "description": "Fast open source Slack desktop app",
  "build": {
    "appId": "org.yue.wey",
    "productName": "Wey",
    "copyright": "Copyright Â© 2018 Cheng Zhao",
    "unpack": "+(*.node|*.html|*.png|*.gif)"
  },
  "bin": {
    "wey": "./scripts/start.js"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "install": "fetch-yode",
    "build": "yackage build out --cache-dir yode",
    "dist": "yackage dist out --cache-dir yode"
  },
  "engines": {
    "node": "^10",
    "yode": "0.4.2"
  },
  "license": "GPL-3.0",
  "homepage": "https://github.com/yue/wey",
  "repository": {
    "type": "git",
    "url": "https://github.com/yue/wey.git"
  },
  "bugs": {
    "url": "https://github.com/yue/wey/issues"
  },
  "dependencies": {
    "@slack/client": "4.8.0",
    "axios": "^0.19.0",
    "binary-search-bounds": "2.0.4",
    "download-yue": "2.x",
    "fetch-yode": "0.2.0",
    "fs-extra": "5.0.0",
    "gui": "0.6.3",
    "keytar": "4.3.0",
    "mini-signals": "1.1.1",
    "opn": "5.3.0",
    "readable-size": "0.2.0",
    "sanitize-html": "1.20.0",
    "string-replace-async": "1.2.1"
  },
  "devDependencies": {
    "yackage": "0.2.10"
  }
}
